<h1>Help Requests</h1>

<table class="table table-hover">
  <thead class="table-primary">
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Description</th>
      <th>Priority</th>
    </tr>
  </thead>
  <tbody>
    <% help_requests.each do |r| %>
      <tr>
        <td><%= r.name %></td>
        <td><%= r.email %></td>
        <td><%= r.description %></td>
        <td><%= r.priority %></td>
      </tr>
    <% end %>
  </tbody>
</table>
